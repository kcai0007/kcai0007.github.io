@model IEnumerable<FIT5120_Waste.Models.WasteKind>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.wasteKindName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.wasteKindName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.wasteKindId }) |
            @Html.ActionLink("Details", "Index","Articles") |
            @Html.ActionLink("Delete", "Delete", new { id=item.wasteKindId })
        </td>
    </tr>
}

</table>
